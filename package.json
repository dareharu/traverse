{
  "$schema": "https://raw.githubusercontent.com/SchemaStore/schemastore/master/src/schemas/json/package.json",
  "name": "@dareharu/traverse",
  "version": "0.1.0",
  "description": "Outputs JSON by visiting, traversing, transforming, and applying every node in template JSON.",
  "main": "dist/index.js",
  "module": "dist/index.mjs",
  "types": "dist/index.d.ts",
  "exports": {
    ".": {
      "import": "dist/index.mjs",
      "require": "dist/index.js",
      "types": "dist/index.d.ts"
    },
    "./traverse": {
      "import": "dist/Traverse.mjs",
      "require": "dist/Traverse.js",
      "types": "dist/Traverse.d.ts"
    }
  },
  "author": "Sayakie <sayakie@kakao.com>",
  "contributors": [],
  "license": "MIT",
  "repository": {
    "type": "git",
    "url": "git+https://github.com/dareharu/traverse.git"
  },
  "bugs": {
    "url": "https://github.com/dareharu/traverse/issues"
  },
  "engines": {
    "node": ">=v16.17.0",
    "pnpm": ">=7 <8"
  },
  "scripts": {
    "lint": "eslint src test --ext ts --fix",
    "format": "prettier --write {src,test}/**/*.ts",
    "docs": "typedoc",
    "bump": "cliff-jumper",
    "check-update": "cliff-jumper --dry-run",
    "test": "vitest run",
    "test:watch": "vitest watch",
    "test:ui": "vitest --ui",
    "commit": "git-cz",
    "cz": "git-cz",
    "preinstall": "npx only-allow pnpm",
    "prepare": "is-ci || husky install .github/husky"
  },
  "packageManager": "pnpm@^7.18.1",
  "publishConfig": {
    "access": "public"
  },
  "config": {
    "commitizen": {
      "path": "./node_modules/cz-conventional-changelog"
    }
  },
  "dependencies": {
    "lodash.clonedeep": "^4.5.0",
    "tslib": "^2.4.1"
  },
  "devDependencies": {
    "@commitlint/cli": "^17.3.0",
    "@commitlint/config-angular": "^17.3.0",
    "@favware/cliff-jumper": "^1.9.0",
    "@sapphire/ts-config": "^3.3.4",
    "@types/lodash.clonedeep": "^4.5.7",
    "@types/node": "^18.11.11",
    "@typescript-eslint/eslint-plugin": "^5.45.1",
    "@typescript-eslint/parser": "^5.45.1",
    "@vitest/ui": "^0.25.5",
    "cz-conventional-changelog": "^3.3.0",
    "eslint": "^8.29.0",
    "eslint-config-prettier": "^8.5.0",
    "eslint-plugin-eslint-comments": "^3.2.0",
    "eslint-plugin-import": "^2.26.0",
    "eslint-plugin-node": "^11.1.0",
    "eslint-plugin-prettier": "^4.2.1",
    "eslint-plugin-simple-import-sort": "^8.0.0",
    "eslint-plugin-sort-keys-fix": "^1.1.2",
    "husky": "^8.0.2",
    "is-ci": "^3.0.1",
    "lint-staged": "^13.1.0",
    "prettier": "^2.8.1",
    "pretty-quick": "^3.1.3",
    "typedoc": "^0.23.21",
    "typedoc-plugin-mdn-links": "^2.0.0",
    "typescript": "^4.9.4",
    "vitest": "^0.25.5"
  }
}
